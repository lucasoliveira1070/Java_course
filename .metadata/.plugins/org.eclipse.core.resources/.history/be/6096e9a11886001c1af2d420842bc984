package entities;

public class Product {
	public String name;
	public double price;
	public int quantity;
	
	public double TotalValueInStock() {
		return quantity * price;
	}
	
	public void addProducts(int qt) {
		quantity += qt;
	}
	
	public void removeProducts(int qt) {
		quantity -= qt;
	}
	
	
	

}
